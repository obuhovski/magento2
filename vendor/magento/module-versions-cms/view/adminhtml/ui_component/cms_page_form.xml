<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="hierarchy">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="collapsible" xsi:type="boolean">true</item>
                <item name="label" xsi:type="string" translate="true">Hierarchy</item>
                <item name="sortOrder" xsi:type="number">40</item>
            </item>
        </argument>
        <container name="hierarchy_container" >
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">Parent page</item>
                </item>
            </argument>
            <htmlContent name="html_content">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="cssclass" xsi:type="string">admin__field</item>
                    </item>
                </argument>
                <argument name="block" xsi:type="object">Magento\VersionsCms\Block\Adminhtml\Cms\Page\Edit\Tab\Hierarchy</argument>
            </htmlContent>
        </container>
    </fieldset>
</form>
